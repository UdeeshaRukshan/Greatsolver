#include<stdio.h>
int main(void)
{int number;

printf("Enter number:");//display Enter number :

scanf("%d",&number);//Get user input from here 42319

printf("%d\t",number/10000);  //display 4

printf("%d\t",number/1000%10);//display 2

printf("%d\t",number/100%10); //display 3

printf("%d\t",number/10%10);  //display 1
  
printf("%d",number%10);       //display 9


    return 0;           //End of the programme
}
